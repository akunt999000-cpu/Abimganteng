-- Library Rayfield
local Rayfield = loadstring(game:HttpGet("https://sirius.menu/rayfield"))()

local Window = Rayfield:CreateWindow({
    Name = "ABIMS V23 | BYPASS EDITION", -- Nama GUI Bersih
    LoadingTitle = "ðŸš€ INITIALIZING SYSTEM",
    LoadingSubtitle = "Preparing Assets Data...",
    ConfigurationSaving = { Enabled = false },
    KeySystem = false,
})

local LocalPlayer = game:GetService("Players").LocalPlayer
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local FS = ReplicatedStorage:WaitForChild("FishingSystem")

-- KEY DISIMPAN SECARA INTERNAL (TIDAK TAMPIL DI GUI)
local _PRIVATE_KEY = "23462347G8235"
local MainRemote = FS:WaitForChild(_PRIVATE_KEY)

_G.Cooldown = false

local function InjectAsset(name, weight, rarity)
    if _G.Cooldown then return end
    _G.Cooldown = true

    -- Pre-hook Simulation
    if FS:FindFirstChild("CleanupCast") then FS.CleanupCast:FireServer() end
    task.wait(2.5) 
    if FS:FindFirstChild("FishingCatchSuccess") then FS.FishingCatchSuccess:FireServer() end
    task.wait(0.5)

    -- Injeksi Nama
    local success, err = pcall(function()
        MainRemote:FireServer({
            ["name"] = name,
            ["weight"] = weight,
            ["rarity"] = rarity,
            ["hookPosition"] = LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(0, -10, 0),
            ["isSpecial"] = true
        })
    end)

    if success then
        Rayfield:Notify({Title = "BERHASIL", Content = name .. " telah masuk ke Tas!", Duration = 3})
    else
        warn("Error: " .. tostring(err))
    end

    task.wait(6.5)
    _G.Cooldown = false
end

-- TAB 1: SECRET & DEVIL
local TabSecret = Window:CreateTab("ðŸ”¥ Secret & Devil", nil)
TabSecret:CreateSection("Target High Value")

local secretList = {
    {"Devil Kraken", 360, "Secret"},
    {"Devil Naga Hitam", 360, "Secret"},
    {"Devil Bone Whale", 350, "Secret"},
    {"Devil Shark Bone", 340, "Secret"},
    {"Shadow Megalodon", 360, "Secret"},
    {"Shadow Crocodile", 330, "Secret"},
    {"Ancient Whale", 360, "Secret"},
    {"Ancient Relic Crocodile", 350, "Secret"},
    {"purple Kraken", 360, "Secret"},
    {"Wraithfin Abyssal", 340, "Secret"}
}

for _, fish in pairs(secretList) do
    TabSecret:CreateButton({
        Name = "Dapatkan " .. fish[1],
        Callback = function() InjectAsset(fish[1], fish[2], fish[3]) end
    })
end

-- TAB 2: MITOS & UNIQUE
local TabMitos = Window:CreateTab("ðŸŒŸ Mitos & Unique", nil)
TabMitos:CreateSection("Target Rare")

local mitosList = {
    {"ArapaimaFish", 220, "Mitos"},
    {"Devilish Gran Maja", 250, "Mitos"},
    {"Devilish Lochness", 240, "Mitos"},
    {"Lochiness Monster", 260, "Mitos"},
    {"Morning Star", 200, "Mitos"},
    {"Sapu Sapu Goib", 180, "Mitos"},
    {"Genetik Fish", 170, "Mitos"}
}

for _, fish in pairs(mitosList) do
    TabMitos:CreateButton({
        Name = "Dapatkan " .. fish[1],
        Callback = function() InjectAsset(fish[1], fish[2], fish[3]) end
    })
end

-- SETTINGS
local TabSet = Window:CreateTab("Settings", nil)
TabSet:CreateButton({
    Name = "RESET COOLDOWN",
    Callback = function() _G.Cooldown = false end
})

Rayfield:Notify({Title = "System Ready", Content = "Silahkan pilih target anda!", Duration = 5})
