-- Library Rayfield
local Rayfield = loadstring(game:HttpGet("https://sirius.menu/rayfield"))()

local Window = Rayfield:CreateWindow({
    Name = "ABIMS FINAL | ULTRA ASSETS",
    LoadingTitle = "ðŸš€ SYNCING ALL FISH DATA",
    LoadingSubtitle = "Remote: 23462347G8235 Active",
    ConfigurationSaving = { Enabled = false },
    KeySystem = false,
})

local LocalPlayer = game:GetService("Players").LocalPlayer
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local FS = ReplicatedStorage:WaitForChild("FishingSystem")
local MainRemote = FS:WaitForChild("23462347G8235")

_G.Cooldown = false
_G.AutoCatch = false
_G.CaughtCount = 0

-- FUNGSI INJECT IKAN
local function InjectAsset(name, weight, rarity)
    if _G.Cooldown then return end
    
    local boosterWasOn = _G.AutoCatch
    _G.AutoCatch = false 
    _G.Cooldown = true

    if FS:FindFirstChild("CleanupCast") then FS.CleanupCast:FireServer() end
    task.wait(0.5)
    
    if FS:FindFirstChild("FishingCatchSuccess") then 
        FS.FishingCatchSuccess:FireServer() 
    end
    
    task.wait(0.2)

    pcall(function()
        MainRemote:FireServer({
            ["name"] = name,
            ["weight"] = weight,
            ["rarity"] = rarity,
            ["hookPosition"] = LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(0, -5, 0),
            ["isSpecial"] = true
        })
    end)

    Rayfield:Notify({Title = "SUCCESS", Content = name .. " Masuk ke Tas!", Duration = 3})
    
    task.wait(2)
    _G.Cooldown = false
    
    if boosterWasOn then
        _G.AutoCatch = true
        task.spawn(function()
            while _G.AutoCatch do
                pcall(function()
                    ReplicatedStorage.FishingCatchSuccess:FireServer()
                    MainRemote:FireServer({{
                        ["name"] = "Blue Fish", 
                        ["rarity"] = "Common",
                        ["weight"] = math.random(5, 15),
                        ["hookPosition"] = LocalPlayer.Character.HumanoidRootPart.Position
                    }})
                    _G.CaughtCount = _G.CaughtCount + 1
                end)
                task.wait(0.01)
            end
        end)
    end
end

-- TAB 1: CAUGHT BOOSTER
local TabStats = Window:CreateTab("ðŸ“ˆ Statistics", nil)
TabStats:CreateSection("âš¡ Leaderboard Auto Catch")
local LabelCount = TabStats:CreateLabel("Caught Session: 0")

task.spawn(function()
    while true do
        LabelCount:Set("Caught Session: " .. _G.CaughtCount)
        task.wait(0.1)
    end
end)

TabStats:CreateToggle({
    Name = "Start Caught Booster",
    CurrentValue = false,
    Flag = "ToggleBooster",
    Callback = function(Value)
        _G.AutoCatch = Value
        if Value then
            task.spawn(function()
                while _G.AutoCatch do
                    pcall(function()
                        if ReplicatedStorage:FindFirstChild("FishingCatchSuccess") then
                            ReplicatedStorage.FishingCatchSuccess:FireServer()
                        end
                        MainRemote:FireServer({{
                            ["name"] = "Blue Fish", 
                            ["rarity"] = "Common",
                            ["weight"] = math.random(5, 15),
                            ["hookPosition"] = LocalPlayer.Character.HumanoidRootPart.Position
                        }})
                        _G.CaughtCount = _G.CaughtCount + 1
                    end)
                    task.wait(0.01)
                end
            end)
        end
    end,
})

-- TAB 2: SEMUA IKAN (SEKARANG SEMUA SECRET)
local TabSecret = Window:CreateTab("ðŸ”¥ All Secret", nil)
TabSecret:CreateSection("Target High Value (Semua Rarity: Secret)")

local secretList = {
    -- Gabungan SynceHub & Assets Original Secret
    {"El Maja", 998, "Secret"},
    {"El Maja Pink", 998, "Secret"},
    {"Kraken", 998, "Secret"},
    {"Mega Hunt", 998, "Secret"},
    {"Lochness Monster", 998, "Secret"},
    {"purple Kraken", 998, "Secret"},
