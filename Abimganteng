    {"KingJally Strong", 998, "Secret"},
    {"King Crab", 998, "Secret"},
    {"Shark Bone", 998, "Secret"},
    {"Naga", 998, "Secret"},
    {"Worm Fish", 998, "Secret"},
    {"Ancient Whale", 998, "Secret"},
    {"Jungle Crocodile", 998, "Secret"},
    {"Devil Kraken", 360, "Secret"},
    {"Devil Naga Hitam", 360, "Secret"},
    {"Devil Bone Whale", 350, "Secret"},
    {"Shadow Megalodon", 360, "Secret"},
    {"Shadow Crocodile", 330, "Secret"},
    {"Ancient Relic Crocodile", 350, "Secret"},
    
    -- Ikan yang sebelumnya Mitos (Sekarang diubah jadi Secret)
    {"Devilish Gran Maja", 998, "Secret"},
    {"Devilish Lochness", 998, "Secret"},
    {"Sapu Sapu Goib", 998, "Secret"},
    {"ArapaimaFish", 220, "Secret"},
    {"Morning Star", 200, "Secret"},
