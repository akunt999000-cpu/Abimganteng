-- Library Rayfield
local Rayfield = loadstring(game:HttpGet("https://sirius.menu/rayfield"))()

-- Inisialisasi Window
local Window = Rayfield:CreateWindow({
    Name = "ABIMS GANTENG V14.0 | SUPREME ARCHITECT",
    LoadingTitle = "üöÄ INITIALIZING ABIMS GANTENG PROTOCOL",
    LoadingSubtitle = "V14.0 Absolute Security Active",
    ConfigurationSaving = {
        Enabled = false,
        FolderName = nil,
        FileName = "AbimsGantengHub"
    },
    Discord = {
        Enabled = false,
        Invite = "",
        RememberJoins = true
    },
    KeySystem = false, -- Tanpa sistem Key
})

local LocalPlayer = game:GetService("Players").LocalPlayer
local ReplicatedStorage = game:GetService("ReplicatedStorage")
_G.MassActive = false
_G.ManualCooldown = false

-- FUNGSI INJECT LENGKAP (DENGAN ANTI-SPAM DELAY)
local function SafeInject(name, weight, vector, rarity)
    if _G.ManualCooldown then 
        Rayfield:Notify({Title = "Sistem", Content = "Tunggu cooldown selesai (5-6 detik)!", Duration = 2})
        return 
    end
    
    _G.ManualCooldown = true
    local FishingSystem = ReplicatedStorage:WaitForChild("FishingSystem", 5)
    local Remote = FishingSystem:WaitForChild("893478GDBSDUAYGDTAF", 5)
    
    -- Simulasi durasi memancing agar aman
    task.wait(math.random(1.2, 2.0)) 
    
    if FishingSystem:FindFirstChild("CleanupCast") then
        FishingSystem.CleanupCast:FireServer()
    end
    
    task.wait(0.5)
    
    if FishingSystem:FindFirstChild("FishingCatchSuccess") then
        FishingSystem.FishingCatchSuccess:FireServer()
    end

    task.wait(0.6)

    local hookPos = LocalPlayer.Character.HumanoidRootPart.Position + vector
    Remote:FireServer({
        hookPosition = hookPos,
        rarity = rarity,
        name = name,
        weight = weight,
    })

    Rayfield:Notify({Title = "Success", Content = "Entitas " .. name .. " disinkronkan.", Duration = 2})
    
    task.wait(6.5) 
    _G.ManualCooldown = false
end

-- TABS CATEGORY
