-- Library Rayfield
local Rayfield = loadstring(game:HttpGet("https://sirius.menu/rayfield"))()

local Window = Rayfield:CreateWindow({
    Name = "ABIMS SANGE | ULTRA ASSETS",
    LoadingTitle = "ðŸš€ ALL FISH DATA",
    LoadingSubtitle = "Remote: Pemancing Active",
    ConfigurationSaving = { Enabled = false },
    KeySystem = false,
})

local LocalPlayer = game:GetService("Players").LocalPlayer
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local FS = ReplicatedStorage:WaitForChild("FishingSystem")
local MainRemote = FS:WaitForChild("23462347G8235")

_G.Cooldown = false
_G.AutoCatch = false
_G.AutoGB = false 
_G.CaughtCount = 0

-- FUNGSI INJECT IKAN (Original logic preserved)
local function InjectAsset(name, weight, rarity)
    if _G.Cooldown then return end
    
    local boosterWasOn = _G.AutoCatch
    _G.AutoCatch = false 
    _G.Cooldown = true

    if FS:FindFirstChild("CleanupCast") then FS.CleanupCast:FireServer() end
    task.wait(0.05)
    
    if FS:FindFirstChild("FishingCatchSuccess") then 
        FS.FishingCatchSuccess:FireServer() 
    end
    
    task.wait(0.01)

    pcall(function()
        MainRemote:FireServer({
            ["name"] = name,
            ["weight"] = weight,
            ["rarity"] = rarity,
            ["hookPosition"] = LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(0, -5, 0),
            ["isSpecial"] = true
        })
    end)

    Rayfield:Notify({Title = "SUCCESS", Content = name .. " Masuk!", Duration = 0.5})
    
    task.wait(0.2)
    _G.Cooldown = false
    
    if boosterWasOn then
        _G.AutoCatch = true
        task.spawn(function()
            while _G.AutoCatch do
                pcall(function()
                    ReplicatedStorage.FishingCatchSuccess:FireServer()
                    MainRemote:FireServer({{
                        ["name"] = "Blue Fish", 
                        ["rarity"] = "Common",
                        ["weight"] = math.random(5, 15),
                        ["hookPosition"] = LocalPlayer.Character.HumanoidRootPart.Position
                    }})
                    _G.CaughtCount = _G.CaughtCount + 1
                end)
                task.wait(0.001)
            end
        end)
    end
end

-- DAFTAR IKAN LENGKAP
