-- TAB 1: STATISTICS & BOOSTER
local TabStats = Window:CreateTab("ðŸ“ˆ Statistics", nil)
TabStats:CreateSection("âš¡ Leaderboard Auto Catch")
local LabelCount = TabStats:CreateLabel("Caught Session: 0")

task.spawn(function()
    while true do
        LabelCount:Set("Caught Session: " .. _G.CaughtCount)
        task.wait(0.1)
    end
end)

TabStats:CreateToggle({
    Name = "Start Caught Booster",
    CurrentValue = false,
    Flag = "ToggleBooster",
    Callback = function(Value)
        _G.AutoCatch = Value
        if Value then
            task.spawn(function()
                while _G.AutoCatch do
                    pcall(function()
                        if ReplicatedStorage:FindFirstChild("FishingCatchSuccess") then
                            ReplicatedStorage.FishingCatchSuccess:FireServer()
                        end
                        MainRemote:FireServer({{
                            ["name"] = "Blue Fish", 
                            ["rarity"] = "Common",
                            ["weight"] = math.random(5, 15),
                            ["hookPosition"] = LocalPlayer.Character.HumanoidRootPart.Position
                        }})
                        _G.CaughtCount = _G.CaughtCount + 1
                    end)
                    task.wait(0.0001) -- Dipercepat maksimal
                end
            end)
        end
    end,
})

-- TAB 2: AUTO GB (BRUTAL INSTANT MODE)
local TabGB = Window:CreateTab("ðŸŒ€ Auto GB", nil)
TabGB:CreateSection("âš¡ ULTRA FAST INSTANT")

TabGB:CreateToggle({
    Name = "Start Instant GB All Secret",
    CurrentValue = false,
    Flag = "ToggleGB",
    Callback = function(Value)
        _G.AutoGB = Value
        if Value then
            task.spawn(function()
                while _G.AutoGB do
                    for _, v in pairs(secretList) do
                        if not _G.AutoGB then break end
                        -- Eksekusi instan tanpa menunggu cooldown fungsi InjectAsset
                        task.spawn(function()
                            pcall(function()
                                ReplicatedStorage.FishingCatchSuccess:FireServer()
                                MainRemote:FireServer({
                                    ["name"] = v[1],
                                    ["weight"] = v[2],
                                    ["rarity"] = v[3],
                                    ["hookPosition"] = LocalPlayer.Character.HumanoidRootPart.Position,
                                    ["isSpecial"] = true
                                })
                                _G.CaughtCount = _G.CaughtCount + 1
                            end)
                        end)
                    end
                    task.wait(0.01) -- Delay antar cycle agar tidak crash
                end
            end)
        end
    end,
})

-- TAB 3: SEMUA IKAN (MANUAL)
local TabSecret = Window:CreateTab("ðŸ”¥ All Secret", nil)
TabSecret:CreateSection("Target High Value (Semua Rarity: Secret)")

for _, fish in pairs(secretList) do
    TabSecret:CreateButton({
        Name = fish[1],
        Callback = function() InjectAsset(fish[1], fish[2], fish[3]) end
    })
end

-- TAB 4: SETTINGS
local TabSet = Window:CreateTab("Settings", nil)
TabSet:CreateButton({
    Name = "RESET COOLDOWN",
    Callback = function() _G.Cooldown = false end
})

-- ANTI-DETECTION
if syn and syn.protect_gui then
    syn.protect_gui(game:GetService("CoreGui").Rayfield)
end

Rayfield:Notify({Title = "READY", Content = "GB Secret & Booster sekarang berjalan Instan!", Duration = 5})
